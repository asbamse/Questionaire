<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="questionaire.MainWindowController">
    <children>
        <Button fx:id="button" layoutX="23.0" layoutY="114.0" onAction="#handleButtonAction" text="Fill Questionaire" />
      <TextField fx:id="txtNameField" layoutX="12.0" layoutY="62.0" prefHeight="25.0" prefWidth="124.0" />
      <Label layoutX="23.0" layoutY="35.0" text="Name" />
      <TableView layoutX="144.0" prefHeight="200.0" prefWidth="181.0">
        <columns>
          <TableColumn fx:id="tableName" prefWidth="90.0" text="Name" />
          <TableColumn fx:id="tableScore" prefWidth="90.0" text="Score" />
        </columns>
      </TableView>
    </children>
</AnchorPane>
